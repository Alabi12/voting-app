<h1>Create Developer</h1>

<%= form_for(@developer, url: admin_developers_path) do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>

  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>

  <div>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
  </div>

  <div>
    <%= f.submit 'Create Developer' %>
  </div>
<% end %>

<%= link_to 'Back', admin_dashboard_path %>
